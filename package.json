{
  "name": "rentzambia",
  "version": "1.0.0",
  "private": true,
  "description": "A modern property rental platform for Zambia",
  "keywords": [
    "property",
    "rental",
    "zambia",
    "nextjs",
    "react",
    "typescript",
    "tailwindcss"
  ],
  "homepage": "https://rentzambia.com",
  "bugs": {
    "url": "https://github.com/yourusername/rentzambia/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/rentzambia.git"
  },
  "license": "MIT",
  "author": "George Jackson <george@rentzambia.com>",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:a11y": "jest --testPathPattern=__tests__/a11y",
    "test:ci": "jest --ci --coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf .next out coverage",
    "clean:deps": "rm -rf node_modules package-lock.json",
    "reinstall": "npm run clean:deps && npm install",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "commit": "git-cz",
    "release": "semantic-release",
    "size": "size-limit",
    "analyze": "ANALYZE=true next build",
    "knip": "knip",
    "ncu": "ncu",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase": "^12.2.1",
    "next": "^15.5.2",
    "next-auth": "^4.24.7",
    "react": "^19",
    "react-dom": "^18",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@next/bundle-analyzer": "^14.1.0",
    "@semantic-release/github": "^10.0.2",
    "@size-limit/preset-app": "^11.1.2",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "autoprefixer": "^10.4.18",
    "commitizen": "^4.3.0",
    "cypress": "^13.6.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8",
    "eslint-config-next": "^15.5.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.1.0",
    "lint-staged": "^15.2.2",
    "npm-check-updates": "^16.14.15",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "semantic-release": "^23.0.2",
    "size-limit": "^11.1.2",
    "tailwindcss": "^4.1.12",
    "ts-jest": "^29.1.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
